# Fit the model:
m2 <- bam(Y ~ Group + s(Condition, k=5)
+ te(Time, Trial, by=Group)
+ s(Time, Subject, bs='fs', m=1, k=5),
data=simdat, rho=rho, AR.start=simdat$start.event,
method='fREML')
save(m2, file='vignette-m2.rda', compress='xz')
